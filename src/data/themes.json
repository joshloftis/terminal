{
  "dark": {
    "app": {
      "position": "absolute",
      "width": "100%",
      "height": "100%",
      "backgroundImage": "url(\"https://media.giphy.com/media/oEI9uBYSzLpBK/giphy.gif\")",
      "backgroundRepeat": "no-repeat",
      "backgroundSize": "cover",
      "zIndex": "-1",
      "opacity": "0.02"
    },
    "terminal": {
      "margin": "0 auto",
      "zIndex": "1",
      "height": "100vh",
      "width": "100vw",
      "maxWidth": "100vw"
    },
    "window": {
      "color": "#F4F4F4",
      "borderBottom": "1px solid #25204f",
      "borderRadius": "5px 5px 0 0"
    },
    "field": {
      "color": "#F4F4F4",
      "padding": "1rem",
      "fontWeight": "normal",
      "borderRadius": "0 0 5px 5px"
    },
    "cursor": {
      "animation": "1.02s blink-dark step-end infinite",
      "display": "inline-block",
      "position": "relative",
      "bottom": "-2px",
      "left": "2px",
      "width": "0.5rem",
      "height": "3px"
    }
  }
}